<?xml version="1.0" encoding="UTF-8"?>
<!-- SendEmailMapper.xml @screen common @author shiyang -->
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.chinaplus.common.service.SendEmailService">          
    <select id="getMailSendFlag" parameterType="com.chinaplus.core.bean.BaseParam" resultType="com.chinaplus.common.entity.TnmMailConfig">
        SELECT
            MAIL_SNED_FLAG AS mailSendFlag
        FROM
            TNM_MAIL_CONFIG
    </select>
</mapper>
